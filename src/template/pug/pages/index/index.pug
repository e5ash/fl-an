extends ../../templates/default
block vars
	-
		title = "Главная страница"
		breadcrumbs = [
			['Главная', 'index.html'],
			[title]
		]
block content
	.section.intro.c-white.bgw#intro(data-header-class="--light")
		.case
			.intro__top.row
				.intro__top-text м. Заельцовская #[br]ул. Красный проспект 167 стр 
				.intro__top-text Студии, 1, 2, 3, 4+ - #[br]комнатные квартиры
				.intro__top-text Квартиры #[br]от 4.0 млн ₽
			.section__wrap
				.intro__content
					.intro__title.h1 Расцветай #[br]на красном
					.intro__text.t1
						p Жилой комплекс #[br]«Расцветай на Красном» – #[br]один из крупнейших проектов, #[br]реализуемых в Новосибирске.
			.intro__controls.row
				.intro__down.down
					.down__title Листайте вниз
					.down__icon
						<svg width="16" height="79" viewBox="0 0 16 79" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M7.2929 78.7071C7.68342 79.0976 8.31659 79.0976 8.70711 78.7071L15.0711 72.3431C15.4616 71.9526 15.4616 71.3195 15.0711 70.9289C14.6805 70.5384 14.0474 70.5384 13.6569 70.9289L8 76.5858L2.34315 70.9289C1.95262 70.5384 1.31946 70.5384 0.928935 70.9289C0.538411 71.3195 0.538411 71.9526 0.928935 72.3431L7.2929 78.7071ZM7 4.37114e-08L7 78L9 78L9 -4.37114e-08L7 4.37114e-08Z" fill="white"/></svg>

				.intro__video.video-btn
					.video-btn__title видеоэксурсия
					.video-btn__wrap
						.video-btn__box
							<svg width="17" height="18" viewBox="0 0 17 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M1.25 18C1.05109 18 0.860322 17.921 0.71967 17.7803C0.579018 17.6397 0.5 17.4489 0.5 17.25V0.749994C0.500022 0.619666 0.534006 0.491593 0.5986 0.378399C0.663195 0.265205 0.756171 0.170796 0.868365 0.104479C0.980559 0.0381625 1.1081 0.0022257 1.23841 0.000211652C1.36872 -0.0018024 1.49731 0.0301758 1.6115 0.0929939L16.6115 8.34299C16.7291 8.40774 16.8271 8.50285 16.8954 8.61842C16.9637 8.73398 16.9998 8.86576 16.9998 8.99999C16.9998 9.13423 16.9637 9.266 16.8954 9.38157C16.8271 9.49713 16.7291 9.59225 16.6115 9.65699L1.6115 17.907C1.50076 17.968 1.37641 18 1.25 18Z" fill="white"/></svg>
						.video-btn__rounds
							.video-btn__round
							.video-btn__round
							.video-btn__round
